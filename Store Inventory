CREATE TABLE store_inventory(id INTEGER PRIMARY KEY, name TEXT, quantity INTEGER, aisle INTEGER, shelf INTEGER);

INSERT INTO store_inventory VALUES (1, "lacrosse heads", 100, 3,1);
INSERT INTO store_inventory VALUES (2, "string", 200, 2,1);
INSERT INTO store_inventory VALUES (3, "poles", 100, 3, 2);

CREATE TABLE country (model text primary key, country text);
INSERT INTO country VALUES ("A","BELGIUM");
INSERT INTO country VALUES ("B", "USA");
INSERT INTO country VALUES ("C", "MEXICO");

SELECT * FROM store_inventory
ORDER BY quantity desc;
SELECT * FROM country;

